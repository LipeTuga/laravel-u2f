{
    "name": "lipetuga/laravel-u2f",
    "description": "U2F support for laravel 7",
    "keywords": [
        "u2f",
        "security",
        "otp"
    ],
    "homepage": "https://github.com/LipeTuga/laravel-u2f",
    "license": "MIT",
    "authors": [
        {
            "name": "LAHAXE Arnaud",
            "email": "lahaxe.arnaud@gmail.com",
            "homepage": "http://lahaxe.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2",
        "illuminate/auth": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/view": "^8.0",
        "illuminate/translation": "^8.0",
        "yubico/u2flib-server": "1.0.1"
    },
    "autoload": {
        "psr-4": {
            "Lipetuga\\U2f\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lipetuga\\U2f\\U2fServiceProvider"
            ],
            "aliases": {
                "U2f": "Lipetuga\\U2f\\U2fFacade"
            }
        }
    }
}
