{
    "name": "lahaxearnaud/laravel-u2f",
    "description": "U2F support for laravel 6",
    "keywords": [
        "u2f",
        "security",
        "otp"
    ],
    "homepage": "https://github.com/lahaxearnaud/laravel-u2f",
    "license": "MIT",
    "authors": [
        {
            "name": "LAHAXE Arnaud",
            "email": "lahaxe.arnaud@gmail.com",
            "homepage": "http://lahaxe.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2",
        "laravel/framework": "^6.0",
        "yubico/u2flib-server": "1.0.2"
    },
    "autoload": {
        "psr-4": {
            "Lahaxearnaud\\U2f\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lahaxearnaud\\U2f\\U2fServiceProvider"
            ],
            "aliases": {
                "U2f": "Lahaxearnaud\\U2f\\U2fFacade"
            }
        }
    }
}
